{
  "name": "alfresco-logo",
  "version": "1.0.1",
  "description": "Produce a 25 character wide colored Alfresco logo",
  "main": "index.js",
  "bin": {
    "alflogo": "cli.js"
  },
  "keywords": [
    "alfresco",
    "logo"
  ],
  "author": "Bindu Wavell",
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "1.1.3",
    "strip-ansi": "3.0.1",
    "taketalk": "1.0.0"
  },
  "devDependencies": {
    "eslint": "2.12.0",
    "eslint-config-standard": "6.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "2.0.0",
    "fixme": "0.5.0",
    "ghooks": "2.0.4",
    "npm-check-updates": "3.1.9"
  },
  "config": {
    "ghooks": {
      "pre-commit": "eslint ."
    }
  },
  "scripts": {
    "eslint": "eslint . || echo DONE",
    "fixme": "node -e \"require('fixme')()\"",
    "ncu": "ncu",
